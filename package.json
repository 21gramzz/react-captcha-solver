{
  "name": "react-captcha-solver-poc",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "start": "electron ./dist/main.js",
    "build": "cross-env NODE_ENV=\"development\" webpack && electron ./dist/main.js",
    "clean": "rimraf dist",
    "package:mac": "cross-env NODE_ENV=\"production\" webpack && electron-builder --mac"
  },
  "build": {
    "asar": true,
    "extraMetadata": {
      "main": "./dist/main.js"
    },
    "extends": null,
    "productName": "React Captcha Solver",
    "appId": "react.captca.poc",
    "directories": {
      "output": "./product"
    },
    "files": [
      "./dist/**/*.js",
      "./dist/**/*.map",
      "./dist/public/*.html",
      "./main.js"
    ],
    "mac": {
      "target": {
        "target": "dir",
        "arch": [
          "arm64",
          "x64"
        ]
      }
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@reduxjs/toolkit": "^1.5.0",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "randomstring": "^1.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.4",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.2.3",
    "ts-node": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/randomstring": "^1.1.6",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/react-redux": "^7.1.15",
    "@types/redux": "^3.6.31",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "cross-env": "^7.0.3",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "eslint": "^7.20.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "html-webpack-inline-source-plugin": "1.0.0-beta.2",
    "html-webpack-plugin": "^4.2.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.17",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.2",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  }
}
